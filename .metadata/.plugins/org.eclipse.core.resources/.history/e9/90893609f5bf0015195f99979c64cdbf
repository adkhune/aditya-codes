
public class Fibonacci {
	public static void main(String args[])
	{
		System.out.println("Hi lets do fibonacci");
		System.out.println(getFibonacciRecursive(45));
		System.out.println(getFibonacci(45));
		
	}
	
	public static int getFibonacciRecursive(int n)
	{
		
		if (n == 0)
			return 0;
		else if (n == 1 || n == 2)
			return 1;
		return getFibonacciRecursive(n-1) + getFibonacciRecursive(n-2);
		
	}
	public static int getFibonacci(int n)
	{
		int[] a = new int[n+1];
		if (n > 4)
		{
			for (int f = 5; f<=n; f++)
			{
			a[0]=0;
			a[1]=1;
			a[2]=1;
			a[3]=2;
			a[4]=3;
			a[f]=a[f-1] + a[f-2];
			
			for (int i = 5; i<=n; i++)
				a[i]=a[i-1]+a[i-2];
			}
		}else{
			a[0]=0;
			a[1]=1;
			a[2]=1;
			a[3]=2;
			a[4]=3;
		}
			return a[n];
	}
}
